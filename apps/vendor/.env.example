# Supabase Configuration (Phase 1)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# FastAPI Configuration (Phase 2 - Future)
VITE_FASTAPI_BASE_URL=http://localhost:8000
VITE_FASTAPI_TOKEN=your_fastapi_token_here

# Backend Selection
# Options: 'supabase' (default) | 'fastapi'
VITE_BACKEND_TYPE=supabase

# Feature Flags
VITE_ENABLE_REALTIME=true
VITE_ENABLE_OFFLINE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info

# PWA Settings
VITE_PWA_ENABLED=true
VITE_PWA_OFFLINE_SUPPORT=true
